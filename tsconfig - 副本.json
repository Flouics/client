{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [ "dom", "es2015", "es2017","es2015.promise" ],
    "target": "es5",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "temp/vscode-dist"
  },
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings"
  ],
  "include":[
    "namespaces"
  ]
}